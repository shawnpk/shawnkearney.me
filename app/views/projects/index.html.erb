<h1 id="page_title">Stuff I've Built</h1>

<div id="posts_wrapper" class="skinny_wrapper">
  <% @projects.each do |project| %>
    <div class="post">
      <div class="date"><%= project.created_at.strftime('%A, %b %d') %></div>
      <h2><%= link_to project.title, project %></h2>
      <hr>
    </div><!-- end .post -->
  <% end %>
</div>
